<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
  <form-beans>
    <form-bean name="indexForm" type="mypackage1.indexForm"/>
  </form-beans>
  <action-mappings>
    <action path="/altas" type="mypackage1.indexAltasAction"  name="indexForm" scope="request">
      <forward name="altas" path="/altas.jsp"/>
    </action>
    <action path="/listado" type="mypackage1.indexListadoAction" name="indexForm" scope="request">
      <forward name="listado" path="/listado.jsp"/>
    </action>
  </action-mappings>
  <message-resources parameter="mypackage1.ApplicationResources"/>
</struts-config>